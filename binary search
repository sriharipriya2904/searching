def binary_search(arr, target):
    arr.sort()
    low = 0
    high = len(arr) - 1

    while low <= high:
        mid = (low + high) // 2
        if arr[mid] == target:
            return f"Found at index {mid}"
        elif arr[mid] < target:
            low = mid + 1
        else:
            high = mid - 1

    return "Not found"

arr = [10, 37, 45, 1, 8, 9]
target = int(input("Enter number to find: "))
print(binary_search(arr, target))

